{
  "name": "amphp/php-cs-fixer-config",
  "description": "PHP CS Fixer config for AMP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sascha-Oliver Prolic",
      "email": "saschaprolic@googlemail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Amp\\CodeStyle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AmpTest\\CodeStyle\\": "test"
    }
  },
  "config": {
    "platform": {
      "php": "7.0.0"
    }
  }
}

